    {
      "name": "Run Browser",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/cli.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Run with URL",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/cli.py",
